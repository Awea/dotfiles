# Git
alias g='git'

# Commit (c)
alias gcm='git commit --message'

# Fetch (f)
alias gfa='git fetch --all'
alias gfr='git pull --rebase'

# Push (p)
alias gp='git push'
alias gpF='git push --force'
alias gpA='git push --all && git push --tags'

# Stash (s)
alias gs='git stash'
alias gsp='git stash pop'
